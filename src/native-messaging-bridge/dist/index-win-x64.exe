  src = toNamespacedPath(src)
  dst = toNamespacedPath(dst)

  let err = null
  try {
    const st = await lstat(src)

    if (st.isDirectory()) {
      if (opts.recursive !== true) {
        throw new FileError('is a directory', {
          operation: 'cp',
          code: 'EISDIR',
          path: src
        })
      }

      try {
        await lstat(dst)
      } catch (e) {
        if (e.code === 'ENOENT') {
          await mkdir(dst, { mode: st.mode, recursive: true })
        } else {
          throw e
        }
      }

