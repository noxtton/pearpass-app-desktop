    !wrapped ||
    typeof wrapped !== 'object' ||
    !wrapped.message ||
    typeof wrapped.length !== 'number'
  ) {
    return null
  }

  const messageJson = JSON.stringify(wrapped.message)
  const actualLength = Buffer.from(messageJson).length

  if (actualLength !== wrapped.length) {
    return null
  }

  return wrapped.message
}

