        log('INFO', 'IPC client disconnected')
        this.desktopAppStatus = DESKTOP_APP_STATUS.NOT_RUNNING
        this.ipcClient = null
      })
    } catch (error) {
      log('INFO', `Failed to connect to IPC server: ${error.message}`)
      this.updateDesktopAppStatus(error)

      // Clean up client on failure
      if (this.ipcClient) {
